#include <iostream>
#include <tchar.h>
using namespace std;

int main()
{
	setlocale(0, "");
	int zadanie,A, B, S, i, N, K;
	double C, P, s, f1, f2, f;
	long int a, b;
	
	
	cout << "Введите номер задания: ";
	cin >> zadanie;

	switch (zadanie)
	{
	case 1:
		cout << "Даны целые положительные числа A и B (A < B).\nВывести все целые числа от A до B включительно; при этом каждое число должно выводиться столько раз, каково его значение (например, число 3 выводится 3 раза).\nВведите целое положительное число А = ";
		cin >> A;
		cout << "Введите целое положительно число В = ";
		cin >> B;
		i = A;
		cout << endl;
		for (i; i <= B; i++)
		{
			for (int j = 0; j < i; j++)
			{
				cout << i << " ";
			}
		}
		cin >> S;
		break;
	case 2:
		cout << "Даны положительные числа A и B (A > B).\nНа отрезке длины A размещено максимально возможное количество отрезков длины B (без наложений).\nНе используя операции умножения и деления, найти длину незанятой части отрезка A.\nВведите длину А = ";
		cin >> A;
		cout << "Введите длину В = ";
		cin >> B;
		C = A % B;
		cout << "Длиннa незанятой части = " << C;
		break;
	case 3:
		cout << "Дано целое число N (> 1).\nВывести наименьшее из целых чисел K, для которых сумма 1 + 2 + . . . + K будет больше или равна N, и саму эту сумму.\nВведите целое число (>1) N = ";
		cin >> N;
		K = 0;
		S = 0;
		while (S <= N)
		{
			++K;
			S += K;
		}
		cout << "Наименьшее из целых чисел K = " << K << "\nСумма = " << S;
		break;
	case 4:
		cout << "Начальный вклад в банке равен 1000 руб.\nЧерез каждый месяц размер вклада увеличивается на P процентов от имеющейся суммы (P — вещественное число, 0 < P < 25).\nПо данному P определить, через сколько месяцев размер вклада превысит 1100 руб., и вывести найденное количество месяцев K (целое число) и Начальный вклад в банке равен 1000 руб. Через каждый месяц размер вклада увеличивается на P процентов от имеющейся суммы (P — вещественное число, 0 < P < 25). По данному P определить, через сколько месяцев размер вклада превысит 1100 руб., и вывести найденное количество месяцев K (целое число) и итоговый размер вклада S (вещественное число). S (вещественное число).\n\nВведите процент = ";
		cin >> P;
		K = 1;
		s = 1000;
		while (s <= 1100)
		{
			++K;
			s += s * P / 100;
		}
		cout << "Количество месяцев = " << K << "\nИтоговый размер вклада = " << s;
		break;
	case 5:
		cout << "Даны целые положительные числа A и B.\nНайти их наибольший общий делитель (НОД), используя алгоритм Евклида.\nВведите число А = ";
		cin >> a;
		cout << "Введите число В = ";
		cin >> b;
		while (a!= b) 
		{
			if (a > b) 
			{
				a = a - b;
			}
			else 
			{
				b = b - a;
			}
		}
		cout << "Наибольший общий делитель = " << a;
		break;
	case 6:
		cout << "Дано целое число N (> 1), являющееся числом Фибоначчи: N = FK.\nНайти целое число K — порядковый номер числа Фибоначчи N.\nВведите целое число N (> 1) = ";
		cin >> N;
		f1 = 1;
		f2 = 1;
		f = 0;
		K = 2;
		while (f < N) 
		{
			++K;
			f = f2 + f1;
			f2 = f1;
			f1 = f;
		}
		cout << "Порядковый номер числа " << N << " = " << K;
		break;
	}
	return 0;
}
